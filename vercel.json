{
    "version": 2,
    "builds": [
      {
        "src": "scripts/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "scripts/server.js",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ]
      }
    ]
  }